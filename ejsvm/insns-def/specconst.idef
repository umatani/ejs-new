\inst specconst (Register dst, Immediate imm)

\otherwise \{
/*
   specconst dst imm
    dst : destination register
    imm : immediate value of the special constant
   $dst = imm
 */

  printf("specconst imm = %x\n", imm);
  if (imm == JS_NULL) {
    dst = new_abstract_value(context);
    printf("creating abstract value\n");
  } else
    dst = imm;
\}
